set prompt = "%m: %c â€º "
set complete = enhance

bindkey "^I" complete-word-fwd
bindkey -k down history-search-forward
bindkey -k up history-search-backward

foreach file (~/.dotfiles/tcsh/*.tcsh)
  source $file
end

if (-e ~/.tcshrc.local) then
  source ~/.tcshrc.local
endif

